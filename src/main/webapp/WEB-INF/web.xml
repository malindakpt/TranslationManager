<web-app version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

	<display-name>Spring MVC Application</display-name>

	<!--School Management Start-->

	<servlet>
		<servlet-name>AddLanguage</servlet-name>
		<servlet-class>servlet.AddLanguage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddLanguage</servlet-name>
		<url-pattern>/AddLanguage</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddTranslation</servlet-name>
		<servlet-class>servlet.AddTranslation</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddTranslation</servlet-name>
		<url-pattern>/AddTranslation</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddTranslationToProduct</servlet-name>
		<servlet-class>servlet.AddTranslationToProduct</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddTranslationToProduct</servlet-name>
		<url-pattern>/AddTranslationToProduct</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RemoveTranslationToProduct</servlet-name>
		<servlet-class>servlet.RemoveTranslationToProduct</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RemoveTranslationToProduct</servlet-name>
		<url-pattern>/RemoveTranslationToProduct</url-pattern>
	</servlet-mapping>


	<!--School Management End-->


	<servlet>
		<servlet-name>mvc-dispatcher</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>mvc-dispatcher</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<context-param>
		<description>Location to store uploaded file</description>
		<param-name>file-upload</param-name>
		<param-value>
			c:\Users\malindak\Desktop\IKMAN\
		</param-value>
	</context-param>

</web-app>